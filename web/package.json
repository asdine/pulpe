{
  "name": "Pulpe",
  "version": "0.1.0",
  "scripts": {
    "build:prod": "node -r babel-register ./node_modules/webpack/bin/webpack --env=prod --optimize-minimize --define process.env.NODE_ENV=\"'production'\" --progress --profile --colors",
    "build:dev": "node -r babel-register ./node_modules/webpack/bin/webpack --env=dev --progress --profile --colors",
    "server": "node -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --env=dev"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-react": "^6.9.0",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "react-hot-loader": "^1.3.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-dev-server": "^2.3.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-merge": "^2.6.1",
    "webpack-validator": "^2.3.0"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  }
}
