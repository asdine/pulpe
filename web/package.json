{
  "name": "pulpe",
  "version": "0.1.0",
  "scripts": {
    "build:prod": "node -r babel-register ./node_modules/webpack/bin/webpack --env=prod --optimize-minimize --define process.env.NODE_ENV=\"'production'\" --progress --profile --colors",
    "build:dev": "node -r babel-register ./node_modules/webpack/bin/webpack --env=dev --progress --profile --colors",
    "server": "node -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --env=dev --open --progress --profile --colors",
    "dev": "concurrently --kill-others -r 'pulpe server --templates ./app/templates --dev' 'yarn run server'"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "chunk-manifest-webpack-plugin": "^1.0.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.1",
    "postcss-loader": "^1.2.2",
    "react-hot-loader": "^1.3.1",
    "redux-logger": "^2.8.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.3.3",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-dev-server": "^2.3.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-merge": "^2.6.1"
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "immutable": "^3.8.1",
    "material-design-icons": "^3.0.1",
    "normalizr": "^3.1.0",
    "react": "^15.4.2",
    "react-dnd": "^2.2.3",
    "react-dnd-html5-backend": "^2.2.3",
    "react-dom": "^15.4.2",
    "react-modal": "^1.7.7",
    "react-portal": "^3.1.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-observable": "^0.14.0",
    "rxjs": "^5.2.0",
    "slate": "^0.19.29"
  },
  "excludedFromBuild": [
    "bootstrap"
  ]
}
